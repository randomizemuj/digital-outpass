<h1>Sign Up</h1>

<form action="/users/create" method="POST">
    <input type="text" name="name" placeholder="Your Name" required>
    <input type="number" name="registration_no" placeholder="Your Registration No" required>
    <input type="text" name="block" placeholder="Your Block/Room no" required>
    <input type="text" name="course" placeholder="Your Course/Branch" required>
    <input type="password" name="password" placeholder="Your Password" required>
    <input type="password" name="confirm_password" placeholder="Confirm Password">
    <input type="submit" value="Sign Up">
</form>